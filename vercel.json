{
  "version": 2,
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "Afbeeldingen/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/catalogus_montung.html",
      "dest": "/catalogus_montung.html"
    },
    {
      "src": "/catalogus_no_prices.html",
      "dest": "/catalogus_montung.html"
    },
    {
      "src": "/Afbeeldingen/(.*)",
      "dest": "/Afbeeldingen/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/catalogus.html"
    }
  ]
}
